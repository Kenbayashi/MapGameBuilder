<?import javafx.scene.text.Font?>

<VBox prefHeight="180.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BuilderController">
   <children>
      <AnchorPane prefHeight="60.0" prefWidth="600.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" prefHeight="32.0" text="タイトル: ">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="titleField" layoutX="84.0" layoutY="14.0" prefHeight="32.0" prefWidth="293.0">
               <font>
                  <Font size="14.0" />
               </font>
            </TextField>
         </children>
      </AnchorPane>
      <HBox prefHeight="60.0" prefWidth="600.0">
         <children>
            <HBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="42.0" prefWidth="106.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#blockPrevAction" prefHeight="32.0" prefWidth="32.0" text="←">
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </Button>
                  <ImageView fx:id="blockView" fitHeight="32.0" fitWidth="32.0" pickOnBounds="true" preserveRatio="true">
